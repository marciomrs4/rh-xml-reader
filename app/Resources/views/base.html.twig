                        <span>Usuário</span>
                        <span class="pull-right-container">
                          <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu">
                    {% if is_granted('ROLE_SUPER_ADMIN') %}
                        <li><a href="{{ path('admin_usuario_index') }}"><i class="fa fa-circle-o"></i> Acessos</a></li>
                    {% endif %}
                        <li><a href="{{ path('admin_my_usuario_edit',{ 'id' : app.user.id  }) }}"><i class="fa fa-circle-o"></i> Meu Acesso</a></li>
                    </ul>
